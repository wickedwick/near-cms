{
  "name": "d-cms",
  "private": true,
  "scripts": {
    "build": "yarn build:contract",
    "build:contract": "asb",
    "build:contract:debug": "asb --target debug",
    "build:web": "next build",
    "deploy": "yarn build:contract:debug && near dev-deploy",
    "dev": "yarn build:contract:debug && near dev-deploy && yarn dev:start",
    "nodemo": "nodemon --watch assembly -e ts --exec && yarn dev:start",
    "dev:start": "env-cmd -f ./neardev/dev-account.env next dev",
    "test:start": "env-cmd -f ./neardev/dev-account.env next start",
    "start": "NEAR_ENV=testnet yarn build:contract:debug && near dev-deploy && yarn dev:start",
    "lint": "next lint",
    "export": "next export",
    "test": "jest",
    "test:watch": "jest --watch"
  },
