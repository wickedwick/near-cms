{
  "name": "d-cms",
  "private": true,
  "scripts": {
    "build": "yarn build:contract",
    "build:contract": "asb",
    "build:contract:debug": "asb --target debug",
    "build:web": "next build",
    "deploy": "yarn build:contract:debug && near dev-deploy",
    "dev": "yarn build:contract:debug && near dev-deploy && yarn dev:start",
    "nodemo": "nodemon --watch assembly -e ts --exec && yarn dev:start",
    "dev:start": "env-cmd -f ./neardev/dev-account.env next dev",
    "test:start": "env-cmd -f ./neardev/dev-account.env next start",
    "start": "yarn deploy && yarn test:start",
    "lint": "next lint"
  },
  "dependencies": {
    "big.js": "^6.1.1",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "gun": "^0.2020.1235",
    "jose": "^4.3.8",
    "nanoid": "^3.1.31",
    "near-api-js": "^0.44.2",
    "near-cli": "^3.1.0",
    "near-sdk-as": "^3.2.3",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@graphprotocol/graph-ts": "^0.24.1",
    "@types/big.js": "^6.1.2",
    "@types/cors": "^2.8.12",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.1",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "gh-pages": "^3.2.3",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.11",
    "typescript": "4.5.4"
  }
}
